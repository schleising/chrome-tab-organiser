{
  "name": "Auto Tab Grouper",
  "description": "Automatically groups tabs based on their URLs.",
  "version": "0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup/tabs-popup.html",
    "default_icon": "icons/tabs.png"
  },
  "background": {
    "service_worker": "service-worker/service-worker.js"
  },
  "permissions": [
    "storage",
    "tabs",
    "tabGroups"
  ]
}
